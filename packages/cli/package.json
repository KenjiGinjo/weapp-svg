{
  "name": "@wesvg/cli",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "wesvg": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -b",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "rimraf oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "eslintConfig": {
    "extends": [
      "oclif",
      "oclif-typescript",
      "alloy",
      "alloy/typescript"
    ]
  },
  "eslintIgnore": [
    "dist",
    "build",
    "out"
  ],
  "dependencies": {
    "@oclif/core": "^2.8.11",
    "@oclif/plugin-help": "^5.2.11",
    "fs-extra": "^11.1.1",
    "iconfont-parser": "^1.0.0",
    "lodash": "^4.17.21",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.27",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "oclif": "^3.9.1"
  },
  "oclif": {
    "bin": "wesvg",
    "dirname": "wesvg",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
